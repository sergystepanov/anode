{
  "name": "anode-web",
  "version": "1.0.0",
  "description": "A frontend for anode app",
  "scripts": {
    "start": "parcel serve ./src/index.html",
    "build": "parcel build ./src/index.html --dist-dir ../static/root",
    "watch": "parcel watch ./src/index.html --dist-dir ../static/root",
    "test": "jest --coverage --passWithNoTests"
  },
  "author": "::::::::::::::: <sergystepanov@gmail.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "babel-jest": "^26.3.0",
    "jest": "^26.4.2",
    "parcel": "next"
  },
  "dependencies": {
    "webrtc-adapter": "^7.7.0",
    "webrtc-fw": "^0.3.7"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  }
}
